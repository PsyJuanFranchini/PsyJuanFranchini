*Labels y format
* Encoding: UTF-8.
FORMATS sexo(F4.0).
variable labels  sexo 'Género'.
VALUE LABELS sexo
  0 'fem.'
  1 'masc'.  
FREQUENCIES sexo.

FORMATS estudio(F6.0).
variable labels  estudio 'Nivel de estudios'.
VALUE LABELS estudio
  1 '1rio incompl.'
  2 '1rio compl.'
  3 '2rio incompl.'
  4 '2rio compl.'
  5 '3rio incompl.'
  6 '3rio compl.'
  7 'Univ. incompl.'
  8 'Univ. compl.'.

FORMATS estadocivil(F6.0).
variable labels  estadocivil 'Est. civil'.
VALUE LABELS estadocivil
  1 'casado'
  2 'en pareja (estable)'
  3 'en pareja (inest.)'
  4 'sep/divorciado'
  5 'viudo'
  6 'soltero sin pareja'.
FREQUENCIES estadocivil.

FORMATS hijos(F4.0).
variable labels  hijos '¿tiene hijos?'.
VALUE LABELS hijos
  0 'No'
  1 'Sí'. 
FREQUENCIES hijos.

FORMATS cuantos(F6.0).
variable labels  cuantos 'N° hijos'.
VARIABLE LEVEL cuantos (SCALE).
FREQUENCIES cuantos.

FORMATS vivesolo(F4.0).
variable labels  vivesolo '¿Vives solo?'.
VALUE LABELS vivesolo
  0 'no.'
  1 'sí'.  
MISSING VALUES vivesolo (2 thru 99).
FREQUENCIES vivesolo.



*/LA ZIMET.

*Items Zimet 3.

VARIABLE LEVEL Z3_01 Z3_02 Z3_03 Z3_04 Z3_05 Z3_06 Z3_07 Z3_08 Z3_09 Z3_10 Z3_11 Z3_12 Z3_13 (SCALE).
MISSING VALUES Z3_01 Z3_02 Z3_03 Z3_04 Z3_05 Z3_06 Z3_07 Z3_08 Z3_09 Z3_10 Z3_11 Z3_12 Z3_13 (99).
VALUE LABELS Z3_01 Z3_02 Z3_03 Z3_04 Z3_05 Z3_06 Z3_07 Z3_08 Z3_09 Z3_10 Z3_11 Z3_12 Z3_13 99 'N.R.'.



FREQUENCIES Z3_01 Z3_02 Z3_03 Z3_04 Z3_05 Z3_06 Z3_07 Z3_08 Z3_09 Z3_10 Z3_11 Z3_12 Z3_13.
EXAMINE VARIABLES=Z3_01 Z3_02 Z3_03 Z3_04 Z3_05 Z3_06 Z3_07 Z3_08 Z3_09 Z3_10 Z3_11 Z3_12 Z3_13
  /PLOT HISTOGRAM
  /STATISTICS NONE
  /CINTERVAL 95
  /MISSING LISTWISE
  /NOTOTAL.

*calculo de subescalas.
COMPUTE Z3_Fam5 = MEAN(Z3_03,Z3_04,Z3_08,Z3_11,Z3_13).
COMPUTE Z3_Fam4 = MEAN(Z3_03,Z3_04,Z3_08,Z3_11).
COMPUTE Z3_Ami = MEAN(Z3_06,Z3_07,Z3_09,Z3_12).
COMPUTE Z3_Osig = MEAN(Z3_01,Z3_02,Z3_05,Z3_10).
Execute.
FORMATS Z3_Fam5  Z3_Fam4 Z3_ami Z3_Osig (F6.2).


OLAP CUBES Z3_Fam5  Z3_Fam4 Z3_ami Z3_Osig BY alumno
  /CELLS=COUNT MEAN STDDEV SKEW MIN MAX
  /TITLE='Escalas Z3_imet v3'.


EXAMINE VARIABLES=Z3_Fam5  Z3_Fam4 Z3_ami Z3_Osig
  /PLOT HISTOGRAM
  /STATISTICS NONE
  /CINTERVAL 95
  /MISSING LISTWISE
  /NOTOTAL.

*Escalas Zimet_R.
COMPUTE Z3_Fam5_R = MEAN(Z3_03_R,Z3_04_R,Z3_08_R,Z3_11_R,Z3_13_R).
COMPUTE Z3_Fam4_R = MEAN(Z3_03_R,Z3_04_R,Z3_08_R,Z3_11_R).
COMPUTE Z3_Ami_R = MEAN(Z3_06_R,Z3_07_R,Z3_09_R,Z3_12_R).
COMPUTE Z3_Osig_R = MEAN(Z3_01_R,Z3_02_R,Z3_05_R,Z3_10_R).
Execute.
FORMATS Z3_Fam5_R  Z3_Fam4_R Z3_ami_R Z3_Osig_R (F6.2).



VARIABLE LEVEL Z3A_01 Z3A_02 Z3A_03 Z3A_04 Z3A_05 Z3A_06 Z3A_07 Z3A_08 Z3A_09 Z3A_10 Z3A_11 Z3A_12 Z3A_13 (SCALE).
MISSING VALUES Z3A_01 Z3A_02 Z3A_03 Z3A_04 Z3A_05 Z3A_06 Z3A_07 Z3A_08 Z3A_09 Z3A_10 Z3A_11 Z3A_12 Z3A_13  (99).
VALUE LABELS Z3A_01 Z3A_02 Z3A_03 Z3A_04 Z3A_05 Z3A_06 Z3A_07 Z3A_08 Z3A_09 Z3A_10 Z3A_11 Z3A3 99 'N.R.'.

EXAMINE VARIABLES=Z3A_01 Z3A_02 Z3A_03 Z3A_04 Z3A_05 Z3A_06 Z3A_07 Z3A_08 Z3A_09 Z3A_10 Z3A_11 Z3A_12 Z3A_13
  /PLOT BOXPLOT STEMLEAF HISTOGRAM
  /COMPARE VARIABLES
  /STATISTICS DESCRIPTIVES EXTREME
  /CINTERVAL 95
  /MISSING LISTWISE
  /NOTOTAL.

OLAP CUBES Z3A_01 Z3A_02 Z3A_03 Z3A_04 Z3A_05 Z3A_06 Z3A_07 Z3A_08 Z3A_09 Z3A_10 Z3A_11 Z3A_12 Z3A_13 BY alumno
  /CELLS=COUNT MEAN STDDEV MIN MEDIAN MAX
  /TITLE='ítems Zimet v3'.

EXAMINE VARIABLES=Z3A_01 Z3A_02 Z3A_03 Z3A_04 Z3A_05 Z3A_06 Z3A_07 Z3A_08 Z3A_09 Z3A_10 Z3A_11 Z3A_12 Z3A_13
  /COMPARE VARIABLE
  /PLOT=BOXPLOT
  /STATISTICS=NONE
  /NOTOTAL
  /ID=alumno
  /MISSING=LISTWISE.

FREQUENCIES Z3A_01 Z3A_02 Z3A_03 Z3A_04 Z3A_05 Z3A_06 Z3A_07 Z3A_08 Z3A_09 Z3A_10 Z3A_11 Z3A_12 Z3A_13.

EXAMINE VARIABLES=Z3A_01 Z3A_02 Z3A_03 Z3A_04 Z3A_05 Z3A_06 Z3A_07 Z3A_08 Z3A_09 Z3A_10 Z3A_11 Z3A_12 Z3A_13
